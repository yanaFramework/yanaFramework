<?xml version="1.0" ?>
<!DOCTYPE database SYSTEM "../dtd/database.dtd">
<database>
    <table name="ft">
        <primarykey>ftid</primarykey>
        <declaration>
            <integer
                name="ftid"
                notnull="yes"
            />
            <integer
                name="ftvalue">
                <default>0</default>
            </integer>
        </declaration>
    </table>
    <table name="t">
        <primarykey>tid</primarykey>
        <foreign table="ft">
            <key name="ftid"/>
        </foreign>
        <declaration>
            <string
                name="tid"
                length="32"
                notnull="yes"
            />
            <integer
                name="tvalue">
                <default>0</default>
            </integer>
            <array
                name="ta"
            />
            <bool
                name="tb"
            />
            <float
                name="tf"
                unsigned="yes"
            />
            <integer
                name="ti"
                length="4"
                fixed="yes"
            />
            <reference
                name="ftid"
                notnull="yes"
                column="ftid"
                label="ftid"
            />
        </declaration>
    </table>
    <table name="i">
        <primarykey>iid</primarykey>
        <foreign table="t">
            <key name="iid"/>
        </foreign>
        <declaration>
            <string
                name="iid"
                length="32"
                notnull="yes"
            />
            <array
                name="ta"
            />
        </declaration>
    </table>
</database>
