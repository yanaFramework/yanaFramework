                    <target>Form data has expired</target>
                    <source/>
                </trans-unit>
                <trans-unit id="FormTimeoutWarning.p">
                    <target>The data contained in the submitted form is more than half an hour old. Before you submit this entry, you should check if the provided data is still up to date.</target>
                    <source/>
                </trans-unit>
            </group>
            <group id="MailMessage">
                <trans-unit id="MailMessage.h">
                    <target>E-mail sent</target>
                    <source/>
                </trans-unit>
                <trans-unit id="MailMessage.p">
                    <target>A message has been sent to the entered e-mail address. Please check your e-mail inbox.</target>
                    <source/>
                </trans-unit>
            </group>
