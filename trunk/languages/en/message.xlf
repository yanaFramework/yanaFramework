    <file source-language="en" datatype="html" original="" target-language="de">
        <header/>
        <body>
            <group id="SuccessMessage">
                <trans-unit id="SuccessMessage.h">
                    <target>Your changes have been saved.</target>
                    <source/>
                </trans-unit>
                <trans-unit id="SuccessMessage.p">
                    <target></target>
                    <source/>
                </trans-unit>
            </group>
            <group id="Error">
                <trans-unit id="Error.h">
                    <target>The requested action could not be completed.</target>
                    <source/>
                </trans-unit>
                <trans-unit id="Error.p">
                    <target>Required files may have been removed or may not be accessable at the moment. Please check back later.<it id="br1" pos="open">&lt;br /&gt;</it>If you (even though) continue to get this message, please contact the administrator.</target>
                    <source/>
                </trans-unit>
            </group>
            <group id="LoginRequiredWarning">
                <trans-unit id="LoginRequiredWarning.h">
                    <target>Password required.</target>
                    <source/>
                </trans-unit>
                <trans-unit id="LoginRequiredWarning.p">
                    <target>You are entering a protected area. Please enter your user name and password to proceed.</target>
                    <source/>
                </trans-unit>
            </group>
            <group id="LowPasswordQualityWarning">
                <trans-unit id="LowPasswordQualityWarning.h">
                    <target>Password quality is too low.</target>
                    <source/>
                </trans-unit>
                <trans-unit id="LowPasswordQualityWarning.p">
                    <target>Required action can not be saved. Please use for you password numbers and special characters.</target>
                    <source/>
                </trans-unit>
            </group>
            <group id="InsufficientRightsWarning">
                <trans-unit id="InsufficientRightsWarning.h">
                    <target>Permission denied.</target>
                    <source/>
                </trans-unit>
                <trans-unit id="InsufficientRightsWarning.p">
                    <target>Your login is correct, but the requested action is not available, since you got insufficient permission for this section. Please check your URL. If you feel this is an error and want your settings checked, please inform your administrator.</target>
                    <source/>
                </trans-unit>
            </group>
            <group id="InvalidImageError">
                <trans-unit id="InvalidImageError.h">
