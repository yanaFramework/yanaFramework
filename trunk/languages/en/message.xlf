                    <target>Permission denied</target>
                    <source/>
                </trans-unit>
                <trans-unit id="PasswordExpiredWarning.p">
                    <target>The time limit for changing your password has expired. Please re-request a new password. If you need assistance, contact your administrator for help.</target>
                    <source/>
