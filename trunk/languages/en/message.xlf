                    <target>Changes could not be saved</target>
                    <source/>
                </trans-unit>
                <trans-unit id="EmptySelectionWarning.p">
                    <target>Please select at least one entry for this operation.</target>
                    <source/>
                </trans-unit>
            </group>
            <group id="DbConnectionError">
                <trans-unit id="DbConnectionError.h">
                    <target>An error occurred</target>
                    <source/>
                </trans-unit>
                <trans-unit id="DbConnectionError.p">
                    <target>Unable to establish database connection.<it id="br3" pos="open">&lt;br /&gt;</it>The service may be temporarily unavailable. Please try again later. If you continue to get this message, please report this issue to your administrator.<it id="br4" pos="open">&lt;br /&gt;</it>Administrator: Please check your connection settings on your administration panel. Check if your database server is running and reachable.</target>
                    <source/>
                </trans-unit>
            </group>
